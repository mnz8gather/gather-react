{
  "name": "gather-rc",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky",
    "dev": "pnpm -F=mock-nest -F=vite-react dev",
    "chart:dev": "pnpm -F=chart-react-vite dev",
    "dev-all": "pnpm -r dev",
    ":sub-modules": "pnpm -r exec rm -rf node_modules",
    ":sub-dist": "pnpm -r exec rm -rf dist",
    "clear": "sh ./clear.sh"
  },
  "devDependencies": {
    "husky": "^9.1.6"
  }
}